! ------------------------------------------------------------------------------
! ROFI Color theme
! ------------------------------------------------------------------------------
rofi.color-enabled: true
rofi.color-window: argb:e8303541, argb:00000000, argb:00000000
rofi.color-normal: argb:00303541, #f3f4f5, argb:00eee8d5, argb:00586e75, #5294e2
rofi.color-active: argb:aefdf6e3, #303541, argb:bceee8d5, argb:ae268bd2, #fdf6e3
rofi.color-urgent: argb:79303541, #dc322f, argb:00eee8d5, argb:00dc322f, argb:00fdf6e3


rofi.lines:3
rofi.width: 85
rofi.modi: window,run
rofi.terminal: rofi-sensible-terminal
rofi.ssh-client: ssh
rofi.ssh-command: {terminal} -e "{ssh-client} {host}"
rofi.case-sensitive: false
rofi.eh: 2
rofi.padding: 340
rofi.hide-scrollbar: true
